      <addRoute url="farm2.static.flickr.com"  />
      <addRoute url="farm3.static.flickr.com"  />
      <addRoute url="farm4.static.flickr.com"  />
      <addRoute url="farm5.static.flickr.com"  />
      <addRoute url="meebo.com" />
      <addRoute url="mail.google.com" />
      <addRoute url="picasaweb.google.com" />
    </Routes>
  </urlRouter>

</configuration>